<div class="row">
  <div class="large-10 large-centered columns">
    <div class="form-containers">

    <div class="form-header-container">
      <div class= "large-12 columns form-header">
        <h1 class="text-center">Log in</h1>
      </div>
    </div>

      


    <div class="con-tainer">
        <div class="row">
          <div class="small-12 small-centered colmuns">
            <div class="contact-container">

              <div class="container-left">
                <ul class="list">
                  <li class="contact-left ">
                  
                    <%= link_to 'sign in with google +', "/auth/google", :class => 'button [tiny small large] goog'%>
                  </li>
                  <li class="contact-left ">
                    <%= link_to 'sign in with twitter', "/auth/twitter", :class => 'button [tiny small large] twit'%>
                  </li>
                  <li class="contact-left ">
                    <%= link_to 'sign in with facebook', "/auth/facebook", :class => 'button [tiny small large] faceb'%>
                    <div ></div>
                  </li>
                </ul>
              </div>

              <div class="container-right">
                <ul class="list">
                  <div class="large-10 medium-centered columns">
                    <div class="site-forms">
                      <%= form_tag "/auth/identity/callback" do %>
                        <div class="field">
                          <%= label_tag :auth_key, "Email" %><br/>
                          <%= text_field_tag :auth_key, :placeholder => "Email" %>
                        </div>
                        <div class="field">
                          <%= label_tag :password %><br/>
                          <%= password_field_tag :password %>
                        </div>
                        <div class="actions">
                          <%= submit_tag "Login", :class => 'button [tiny small large] save-button' %>
                        </div>
                      <% end %>
                    </div>
                  </div>
                </ul>
              </div>
              <div class="clearfix"></div>
              </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
